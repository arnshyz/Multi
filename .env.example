# Copy this file to .env and fill in your credentials.

# Gemini API
GEMINI_API_KEY="your_gemini_api_key"

# Freepik API keys can be provided as a comma or newline separated list.
FREEPIK_API_KEYS="your_freepik_key_1,your_freepik_key_2"
# Alternatively, specify numbered variables if you prefer.
FREEPIK_API_KEY_1="your_freepik_key_1"
FREEPIK_API_KEY_2="your_freepik_key_2"

# Optional Redis overrides used by the Freepik integration.
# FREEPIK_REDIS_HOST=127.0.0.1
# FREEPIK_REDIS_PORT=6379
# FREEPIK_REDIS_TIMEOUT=1.5
# FREEPIK_REDIS_PASSWORD=
# FREEPIK_REDIS_DATABASE=0
# FREEPIK_REDIS_KEY=freepik:api-keys
